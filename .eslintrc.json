{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "google",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "new-cap": "off",
        "linebreak-style": "off",
        "comma-dangle": "off",
        "indent": ["warn", 4],
        "require-jsdoc": "off"
    }
}